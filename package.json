{
	"name": "vite-block",
	"version": "1.0.0",
	"contributors": [
		"Erik Golinelli <erik@codekraft.it> (https://codekraft.it/)"
	],
	"license": "GPL-2.0-or-later",
	"files": [
		"build/*",
		"vite-block.php",
		"block.json",
		"LICENSE.md",
		"readme.txt"
	],
	"scripts": {
		"packages-update": "wp-scripts packages-update",
		"start": "tsc && vite build --watch",
		"build": "tsc && vite build",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"wp-env:start": "wp-env start",
		"wp-env:stop": "wp-env stop",
		"wp-env:destroy": "wp-env destroy",
		"plugin-zip": "npm pack plugin-zip"
	},
	"dependencies": {
		"glob": "^10.3.10",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@types/node": "^20.10.6",
		"@types/react": "^18.2.43",
		"@types/react-dom": "^18.2.17",
		"@types/wordpress__block-editor": "^11.5.9",
		"@types/wordpress__blocks": "^12.5.13",
		"@typescript-eslint/eslint-plugin": "^6.14.0",
		"@typescript-eslint/parser": "^6.14.0",
		"@vitejs/plugin-react": "^4.2.1",
		"@wordpress/block-editor": "^12.16.0",
		"@wordpress/blocks": "^12.25.0",
		"eslint": "^8.55.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"sass": "^1.69.7",
		"typescript": "^5.2.2",
		"vite": "^5.0.8"
	},
	"engines": {
		"node": ">=14.0.0",
		"npm": ">=6.9.0"
	},
	"eslintConfig": {
		"extends": [
			"plugin:@wordpress/eslint-plugin/esnext"
		],
		"globals": {
			"window": true,
			"document": true,
			"localStorage": true
		}
	},
	"browserslist": [
		"extends @wordpress/browserslist-config"
	],
	"prettier": "@wordpress/prettier-config"
}
